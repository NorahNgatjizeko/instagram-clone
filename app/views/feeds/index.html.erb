<p id="notice"><%= notice %></p>

<h1>Feeds List</h1>
<br>
<table class="container">
  <div class="row">
    <% @feeds.each do |feed| %>
         <p><%= feed.user.name %></p>
         <p>Content: <%= feed.content %></p>
         <p><%= image_tag(feed.image.url) %></p>
         <tr>
           <button type="button" class="btn btn-info"><%= link_to 'See', feed, data: {confirm: 'Are you sure you want to see?'}%></button>
           <button type="button" class="btn btn-info"><%= link_to 'Edit', edit_feed_path(feed), data: {confirm: 'Are you sure you want to edit?'}%></button>
           <button type="button" class="btn btn-info"><%= link_to 'Delete', feed, method: :delete, data: { confirm: 'Are you sure?' } %></button>
        </tr>
       <% end %>
       </div>
       </table>
 <br>

<button type="button" class="btn btn-info"><%= link_to 'New Feed', new_feed_path %></button>
